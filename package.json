{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite -d",
    "build": "vite build",
    "preview": "vite preview",
    "func": "cd api && func start",
    "swa": "swa start http://localhost:3000",
    "all": "npm-run-all --parallel dev func swa",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^0.8.3",
    "@vitejs/plugin-vue": "^2.3.1",
    "eslint-plugin-vue": "^8.7.1",
    "npm-run-all": "^4.1.5",
    "vite": "^2.9.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
